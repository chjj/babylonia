{
  "name": "es-errors",
  "version": "1.3.0",
  "description": "A simple cache for a few of the JS Error constructors.",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./eval": "./eval.js",
    "./range": "./range.js",
    "./ref": "./ref.js",
    "./syntax": "./syntax.js",
    "./type": "./type.js",
    "./uri": "./uri.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljharb/es-errors.git"
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "error",
    "typeerror",
    "syntaxerror",
    "rangeerror"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ljharb/es-errors/issues"
  },
  "homepage": "https://github.com/ljharb/es-errors#readme",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false,
    "backfillLimit": false,
    "hideCredit": true
  },
  "engines": {
    "node": ">= 0.4"
  }
}
