  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/utils.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "^<rootDir>/test/testharness.js$",
      "^<rootDir>/test/web-platform-tests/"
    ]
  },
  "license": "MIT",
  "main": "lib/public-api.js",
  "name": "whatwg-url",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdom/whatwg-url.git"
  },
  "version": "7.0.0"
}
