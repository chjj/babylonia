  "license": "MIT",
  "lintDeps": {
    "devDependencies": {
      "files": {
        "options": {
          "ignore": [
            "benchmark/**/*.js"
          ]
        }
      }
    }
  },
  "main": "index.js",
  "name": "extglob",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micromatch/extglob.git"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "braces",
        "expand-brackets",
        "expand-range",
        "fill-range",
        "micromatch"
      ]
    },
    "helpers": [
      "helper-changelog"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "version": "2.0.4",
  "bundledDependencies": [
    "define-property",
    "extend-shallow",
    "is-accessor-descriptor",
    "is-data-descriptor",
    "is-descriptor"
  ]
}
