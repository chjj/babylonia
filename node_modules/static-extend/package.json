    "ctor",
    "descriptor",
    "extend",
    "extends",
    "inherit",
    "inheritance",
    "merge",
    "method",
    "prop",
    "properties",
    "property",
    "prototype"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "static-extend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/static-extend.git"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
